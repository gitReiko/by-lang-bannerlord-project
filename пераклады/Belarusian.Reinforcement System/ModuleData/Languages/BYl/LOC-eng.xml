		<string id = "RSMAL021" text = " {?HERO.GENDER}Šlachcianka!{?}Šlachcič!{\?} Na haryzoncie zjaviłasia vojska, i, zdajecca, jano hatovaje dapamahčy!" />  
		<string id = "RSMAL022" text = " {?HERO.GENDER}Pani,{?}Panovie,{\?} ściah {CLAN_NAME} uzdymajecca na haryzoncie. Našy vorah atrymlivaje ŭzmacnieńnie!" />
		<string id = "RSMAL023" text = " {?HERO.GENDER}Haspadarynia!{?}Haspadar!{\?} Na haryzoncie zjaviłasia vojska, jakoje, zdajecca, dałučajecca da voraha." />

		<!--   3. Ally Clan or Enemy Clan  -->
