 <strings>
  <string id="dart_label_str" text="Magic Bolt"/>
  <string id="ward_of_arrows_label_str" text="Ward of Arrows"/>
  <string id="ward_of_arrows_description_str" text="Adds 40% physical resistance against ranged damage for 15 seconds to the caster."/>
  <string id="minor_heal_label_str" text="Petty Heal"/>
  <string id="minor_heal_description_str" text="Heals the caster for 3 health every second for 7 seconds."/>
  <string id="foetid_cloud_label_str" text="Foetid Cloud"/>
  <string id="foetid_cloud_description_str" text="A sickly short ranged poisonous gas cloud."/>
  <string id="enchant_weapon_label_str" text="Enchant Weapon"/>
  <string id="enchant_weapon_description_str" text="Buff a target ally's weapon to do additional magic damage."/>
  <string id="aura_of_resistance_label_str" text="Aura of Resistance"/>
  <string id="aura_of_resistance_description_str" text="Allies in a 3 meter radius gain 20% physical resistance for 15 seconds."/>
  <string id="strength_of_combat_label_str" text="Strength Of Combat"/>
  <string id="strength_of_combat_description_str" text="Adds 20% physical damage bonus for 15 seconds to the caster."/>
  <string id="dust_storm_label_str" text="Dust Storm"/>
  <string id="dust_storm_description_str" text="Dry winds from the desert knock down victims and apply minor damage."/>
  <string id="fireball_label_str" text="Fireball"/>
  <string id="fireball_description_str" text="There is nothing quite as welcoming as a fireball to the face. Deals 70 damage in a 6 meter radius and burns the foes for 5 seconds."/>
  <string id="cinder_blast_label_str" text="Cinder Blast"/>
  <string id="cinder_blast_description_str" text="Hurl fragments of solidified lava at the enemy dealing medium damage on contact."/>
  <string id="bolt_of_aqshy_label_str" text="Bolt of Aqshy"/>
  <string id="bolt_of_aqshy_description_str" text="A fast moving flaming bolt that does medium damage on impact."/>
  <string id="sear_label_str" text="Sear"/>
  <string id="sear_description_str" text="Blast the enemy with a short ranged searing flame attack that does medium damage."/>
  <string id="cascading_fire_cloak_label_str" text="Cascading Fire-Cloak"/>
  <string id="cascading_fire_cloak_description_str" text="The caster wraps himself in a cloak of magical flame that shields him from physical (20%) and fire (70%) damage for 30 seconds. The cloak also reflects a portion of damage taken in melee."/>
  <string id="flaming_swords_label_str" text="Flaming Sword of Rhuin"/>
  <string id="flaming_swords_description_str" text="Buff the weapons of allies in a 15 meter radius with the Flaming trait for 45 seconds. The weapons burst into fire and burn every enemy they strike."/>
  <string id="burning_head_label_str" text="The Burning Head"/>
  <string id="burning_head_description_str" text="Conjure a large burning skull that moves forward burning everything in its path."/>
  <string id="flame_storm_label_str" text="Fire Tornado"/>
  <string id="flame_storm_description_str" text="A randomly moving pillar of searing flame is summoned on the battlefield. Flesh and skin melt, while bones turn to ash."/>
  <string id="lightning_bolt_label_str" text="Lightning Bolt"/>
  <string id="lightning_bolt_description_str" text="Hurl an electric bolt at the enemy that does medium damage on impact."/>
  <string id="wind_blast_label_str" text="Wind Blast"/>
  <string id="wind_blast_description_str" text="A gust of wind knocks down victims and applies minor damage."/>
  <string id="chain_lightning_label_str" text="Chain Lightning"/>
  <string id="chain_lightning_description_str" text="A ball of lightning that zaps enemies in its area for medium damage."/>
  <string id="harmonic_convergence_label_str" text="Harmonic Convergence"/>
  <string id="harmonic_convergence_description_str" text="Divining favorable signs, the caster guides the minds of his fellow warriors. Allies in a 10 meter radius gain 50% physical resistance for 50 seconds."/>
  <string id="blizzard_label_str" text="Iceshard Blizzard"/>
  <string id="blizzard_description_str" text="Stormy clouds loom over the area showering those below with sharp shards of ice."/>
  <string id="comet_of_casandora_label_str" text="Comet of Casandora"/>
  <string id="comet_of_casandora_description_str" text="A large meteor strikes the area killing all below in a giant shockwave."/>
  <string id="thunderbolt_label_str" text="Urannon's Thunderbolt"/>
  <string id="thunderbolt_description_str" text="A single large bolt of lightning crashes down from the skies with a sizeable electric charge."/>
  <string id="curse_of_midnight_wind_label_str" text="Curse of the Midnight Wind"/>
  <string id="curse_of_midnight_wind_description_str" text="The wizard speaks an ancient three-word hex, cursing the enemies and iposing a 40% penalty to movement and attack speed, as well as damage dealt."/>
  <string id="shem_gaze_label_str" text="Shem's Burning Gaze"/>
  <string id="shem_gaze_description_str" text="A bolt of cleansing energy fly from the caster's hands, searing evil wherever it strikes."/>
  <string id="healing_light_label_str" text="Healing Light"/>
  <string id="healing_light_description_str" text="Allies in the area of effect regenerate health over time."/>
  <string id="pha_protection_label_str" text="Pha's Protection"/>
  <string id="pha_protection_description_str" text="The wizard calls upon the beneficent Guardian of Light to protect his allies from harm. Grants 50% physical resistance for a duration 50 seconds."/>
  <string id="deathly_shards_label_str" text="Deathly Shards"/>
  <string id="deathly_shards_description_str" text="Sharp shards of light fly towards the target piercing through the toughest armor."/>
  <string id="radiance_pillar_label_str" text="Pillar of Radiance"/>
  <string id="radiance_pillar_description_str" text="A large pillar of light applies high damage to those caught in the beam."/>
  <string id="radiant_gaze_label_str" text="Radiant Gaze"/>
  <string id="radiant_gaze_description_str" text="Columns of light rise from the ground in a path."/>
  <string id="net_of_amyntok_label_str" text="Net of Amyntok"/>
  <string id="net_of_amyntok_description_str" text="The wizard conjures an arcane net in the selected area. Any foe caught in the spell is halted for 25 seconds."/>
  <string id="bironas_timewarp_label_str" text="Birona's Timewarp"/>
  <string id="bironas_timewarp_description_str" text="Slows down time, while units around you move unhindered."/>
  <string id="nagash_gaze_label_str" text="Gaze of Nagash"/>
  <string id="nagash_gaze_description_str" text="A bolt of Dhar flies from the caster's eyes, and what flesh it touches blackens and withers, peeling away completely to reveal bleached white bone beneath."/>
  <string id="summon_skeleton_label_str" text="Raise the Dead"/>
  <string id="summon_skeleton_description_str" text="Raises old bones from the ground for the Necromancer to command. Summon 25 Skeleton Warriors."/>
  <string id="shadowblood_label_str" text="Shadowblood"/>
  <string id="shadowblood_description_str" text="Shadowy orbs of blood follow the target staining the ground beneath every step of the way. Deals damage in a medium area."/>
